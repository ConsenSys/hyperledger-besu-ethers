  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naddison36/private-ethers/issues"
  },
  "homepage": "https://github.com/naddison36/private-ethers#readme",
  "dependencies": {
    "ethers": ">5.0.0-beta.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.14",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  }
}
